class Solution {
public:
    int majorityElement(vector<int>& nums) {
        map<int,int> mp;
        
        int n=nums.size();
        for(auto it:nums)
        {
            mp[it]++;
            
            if(mp[it] > n/2)
            {
                return it;
                break;
            }
        }
        return 0;
    }
};
