class Solution {
public:
    vector<int> topKFrequent(vector<int>& nums, int k) {
        vector<int> ans;
        
        map<int,int> mp;
        
        for(auto it:nums)
        {
            mp[it]++;
        }
        
        priority_queue<pair<int,int>,vector<pair<int,int>>,greater<pair<int,int>>> pq;
        for(auto it:mp)
        {
            pq.push({it.second,it.first});
            
            if(pq.size() > k)
            {
                pq.pop();
            }
        }
        
        while(!pq.empty())
        {
            int res=pq.top().second;
            pq.pop();
            
            ans.push_back(res);
        }
        return ans;
    }
};
